    "version": "0.2.0",
    "configurations": [
        {
            "name": "kalemni development",
            "request": "launch",
            "type": "dart",
            "program": "lib/main_production.dart",
            "args": [
                "--flavor",
                "production",
                "--target",
                "lib/main_production.dart"
            ]
        },
        {
            "name": "kalemni production",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "program": "lib/main_production.dart",
            "args": [
                "--flavor",
                "production",
                "--target",
                "lib/main_production.dart"
            ]
        },
        {
            "name": "chat_app",
            "request": "launch",
            "type": "dart",
            "program": "lib/main_development.dart",
        },
        {
            "name": "chat_app (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "program": "lib/main_production.dart",
            "toolArgs": [
                "--flavor",
                "production",
                "--target",
                "lib/main_production.dart",
                "--no-enable-impeller"
            ]
        },
        {
            "name": "kalemni Production (build)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "program": "lib/main_production.dart",
            "args": [
                "--flavor",
                "production"
            ]
        }
    ]
}